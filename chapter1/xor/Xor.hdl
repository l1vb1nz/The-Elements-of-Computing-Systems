/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a,out=nega);
    Not(in=b,out=negb);
    And(a=nega,b=b,out=w1);
    And(a=a,b=negb,out=w2);
    Or(a=w1,b=w2,out=out);
    
}